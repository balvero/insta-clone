<div class="col-md-6 col-md-offset-3">
    <div class="center">
        <% @photos.each do |photo| %>
          <div class="photo-list">
            <%= image_tag photo.image.url, class: 'img-responsive' %>
            <h2><%= link_to photo.title, photo_path(photo), class: 'title' %></h2>
          </div>
        <% end %>
    </div>
</div>

